l) {\n            var pattern = new RegExp(/^((\"[\\w-\\s]+\")|([\\w-]+(?:\\.[\\w-]+)*)|(\"[\\w-\\s]+\")([\\w-]+(?:\\.[\\w-]+)*))(@((?:[\\w-]+\\.)*\\w[\\w-]{0,66})\\.([a-z]{2,6}(?:\\.[a-z]{2})?)$)|(@\\[?((25[0-5]\\.|2[0-4][0-9]\\.|1[0-9]{2}\\.|[0-9]{1,2}\\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\\]?$)/i);\n\n            if (!pattern.test(this.state.id )) {\n                is_valid_email = false;\n                message.error(\"Please enter a valid email address\");\n            } else {is_valid_email = true}\n        }\n        if(is_valid_email === true){\n            if(user.id !== 'admin@email.com' || user.password !== 'password'){\n                message.error('You entered wrong email or password')\n            }else {this.props.login(user)}\n        }\n    }\n    render(){\n        const user = {\n            id:this.state.id,\n            password: this.state.password\n        };\n        const is_disabled = this.state.id === null || this.state.password === null,\n               token = this.props.token\n\n        if(token){return <Redirect to={Routes.dashboard}/>}\n\n\n        return(\n           <div className=\"App\">\n               <h1 className=\"welcome-text\">Welcome to Buddies, login</h1>\n               <div className=\"login-form\">\n                   <div>\n                       <b>Email</b>\n                       <Input placeholder=\"Enter eail\"\n                              className=\"lg-input-field\"\n                              onChange={(e)=>this.handleInput(e, 'email')}\n                       />\n                   </div>\n                   <div>\n                       <b>Password</b>\n                       <Input type=\"password\"\n                              placeholder=\"Enter password\"\n                              className=\"lg-input-field\"\n                              onChange={(e)=>this.handleInput(e, 'password')}\n                       />\n                   </div>\n                   <Button disabled={is_disabled} className=\"login-btn\"\n                           onClick={this.login}>\n                       Login\n                   </Button>\n               </div>\n               <div className=\"form-extra\">\n                   <div><large>Forgot password? <a>Reset</a></large></div>\n                   <div><large>Not registered yet? <a style={{color:'orange'}}>SIGN UP</a></large></div>\n               </div>\n           </div>\n        )\n    }\n}\n\nconst mapStateToProps = state => ({\n    token: state.main.token,\n});\n\nconst mapDispatchToProps = dispatch => {\n    return {\n        login: (user) => dispatch(login(user)),\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Login);","/home/vansolo/Desktop/mybuddies/src/redux/reducers/rootReducer.jsx",[],{"ruleId":"65","severity":1,"message":"66","line":4,"column":9,"nodeType":"67","messageId":"68","endLine":4,"endColumn":17},{"ruleId":"65","severity":1,"message":"69","line":7,"column":8,"nodeType":"67","messageId":"68","endLine":7,"endColumn":14},{"ruleId":"65","severity":1,"message":"70","line":12,"column":31,"nodeType":"67","messageId":"68","endLine":12,"endColumn":37},{"ruleId":"65","severity":1,"message":"71","line":51,"column":13,"nodeType":"67","messageId":"68","endLine":51,"endColumn":18},{"ruleId":"65","severity":1,"message":"72","line":52,"column":13,"nodeType":"67","messageId":"68","endLine":52,"endColumn":26},{"ruleId":"65","severity":1,"message":"73","line":57,"column":15,"nodeType":"67","messageId":"68","endLine":57,"endColumn":26},{"ruleId":"74","severity":1,"message":"75","line":75,"column":13,"nodeType":"76","messageId":"77","endLine":75,"endColumn":22},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"65","severity":1,"message":"82","line":8,"column":8,"nodeType":"67","messageId":"68","endLine":8,"endColumn":16},{"ruleId":"65","severity":1,"message":"83","line":12,"column":16,"nodeType":"67",